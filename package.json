		"test:claim:max": "ava __tests__/claims/max-asset-claim.ava.ts",
		"test:passwords": "ava __tests__/passwords/password-tests.ava.ts",

		"test:nft-keys": "ava __tests__/nft-keys/nft-keys-basics.ava.ts",

