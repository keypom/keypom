	"dependencies": {
		"crypto": "^1.0.1",
		"gh-pages": "^4.0.0",
		"@keypom/core": "^1.0.1",
    	"@keypom/selector": "^1.1.0"
	},
	"devDependencies": {
		"@types/bn.js": "^5.1.0",
